{
    "name": "bermudaphp/skeleton",
    "description": "Bermuda app skeleton",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">= 7.4 | >= 8.0",
        "elie29/zend-phpdi-config": "^8.0",
        "laminas/laminas-config-aggregator": "^1.3",
        "bermudaphp/app": "^2.1.2",
        "bermudaphp/config": "^1.0",
        "bermudaphp/http": "^1.0",
        "nyholm/psr7": "^1.4.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.1",
        "laminas/laminas-code": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Console\\": "bin/"
        },
        "files": [
            "app/functions.php"
        ]
    }
}
